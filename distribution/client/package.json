    "coverageThreshold": {
      "global": {
        "statements": 54,
        "branches": 41,
        "functions": 45,
        "lines": 54
      }
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Distribution Client Test Report",
          "outputPath": "build/test-report.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true
